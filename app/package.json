{
  "name": "azure-swa-fullstack-starter",
  "version": "1.0.0",
  "type": "module",
  "description": "Production-ready Azure Static Web Apps template with React, Functions, Cosmos DB, and full CI/CD",
  "main": "dist/functions/*.js",
  "scripts": {
    "dev": "vite --port 5173 --host",
    "dev:api": "cd api && func start --port 7071",
    "dev:swa": "swa start . --api-location api",
    "build": "vite build",
    "build:api": "cd api && npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "type-check": "tsc --noEmit",
    "seed:demo": "node ../scripts/seed-demo-data.js",
    "seed:reset": "node ../scripts/seed-demo-data.js --reset"
  },
  "keywords": [
    "azure",
    "static-web-apps",
    "react",
    "cosmos-db",
    "serverless",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@azure/functions": "^4.9.0",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@fontsource-variable/dm-sans": "^5.0.0",
    "@fontsource-variable/space-grotesk": "^5.0.0",
    "@headlessui/react": "^2.1.0",
    "@heroicons/react": "^2.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@tanstack/react-query": "^5.50.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.440.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.26.0",
    "recharts": "^3.5.1",
    "tailwind-merge": "^2.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
